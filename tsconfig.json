{
    "compilerOptions": {
      "target": "es6",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react",
      "rootDir": "src",
      "baseUrl": "src",
      "paths":{
        "@/*": ["*"]
      },
      "allowJs": true,
      "strict": false,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
    },
    "include": ["src"],
  }